{
    "name": "merkeleon/phpnsq",
    "description": "PHP Client for nsq",
    "type": "library",
    "keywords": ["nsq", "queue"],
    "homepage": "http://github.com/stackerzzq/phpnsq",
    "require": {
        "monolog/monolog": "^2.0",
        "symfony/console": "^5.0",
        "react/event-loop": "^0.4.3",
        "bramus/monolog-colored-line-formatter": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "OkStuff\\PhpNsq\\": "src/phpnsq/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OkStuff\\PhpNsq\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "stackerzzq",
            "email": "foo_stacker@yeah.net"
        }
    ],
    "license": "MIT"
