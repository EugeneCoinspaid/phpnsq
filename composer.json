{
    "name": "merkeleon/phpnsq",
    "description": "PHP Client for nsq",
    "type": "library",
    "keywords": ["nsq", "queue"],
    "homepage": "http://github.com/stackerzzq/phpnsq",
    "require": {
        "monolog/monolog": "^3.6",
        "symfony/console": "^7.1",
        "react/event-loop": "^1.5",
        "bramus/monolog-colored-line-formatter": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    },
    "autoload": {
        "psr-4": {
            "OkStuff\\PhpNsq\\": "src/phpnsq/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OkStuff\\PhpNsq\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "stackerzzq",
            "email": "foo_stacker@yeah.net"
        }
    ],
    "license": "MIT"
}
