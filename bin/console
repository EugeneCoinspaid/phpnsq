#!/usr/bin/env php
<?php
// application.php

require __DIR__.'/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use OkStuff\PhpNsq\Command\Publish;
use OkStuff\PhpNsq\Command\Subscribe;

$application = new Application();

// ... register commands / 注册命令
$application->add(new Publish());
$application->add(new Subscribe());

$application->run();
